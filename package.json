    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack-bundle-analyzer": "^2.11.1"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently \"npm run start-index\" \"npm run start-edit\" \"npm run start-templates\"",
    "start-index": "bisheng start -c ./site/bisheng.index.config.js",
    "start-edit": "bisheng start -c ./site/bisheng.edit.config.js",
    "start-templates": "bisheng start -c ./site/bisheng.templates.config.js",
    "build-index": "bisheng build -c ./site/bisheng.index.config.js",
    "build-edit": "bisheng build -c ./site/bisheng.edit.config.js",
    "build-templates": "bisheng build -c ./site/bisheng.templates.config.js",
    "build": "cross-env NODE_ENV=production concurrently \"npm run build-index\" \"npm run build-edit\" \"npm run build-templates\"",
    "site": "rm -rf _site && npm run build && gh-pages -d _site",
    "deploy": "npm run re-site && npm run site && bisheng gh-pages --push-only",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:demo",
    "lint:es": "eslint --fix ./ --ext '.js,.jsx,.md'",
    "lint:style": "stylelint \"site/**/*.less\" --syntax less",
    "lint:demo": "cross-env RUN_ENV=DEMO eslint ./ --ext '.md' -c ./.eslintrc.js",
    "test": "npm run lint",
    "analyzer": "webpack-bundle-analyzer bundle/output/path/stats.json"
  },
  "theme": "./theme.js"
}
